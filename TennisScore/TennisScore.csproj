<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\images.jpg" />
    <None Remove="Assets\Mas.png" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\TennisLibrary\TennisLibrary.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\Images.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Assets\Mas.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
